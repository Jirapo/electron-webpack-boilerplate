{
  "name": "electron-webpack-boilerplate",
  "version": "1.0.0",
  "description": "laa",
  "main": "./app/dist/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "dev:renderer": "./node_modules/.bin/webpack-dev-server --config ./webpack/renderer.dev.conf.js",
    "dev:main": "./node_modules/.bin/webpack --config ./webpack/main.dev.conf.js",
    "build:main": "./node_modules/.bin/webpack --config ./webpack/main.prod.conf.js",
    "build:renderer": "./node_modules/.bin/webpack --config ./webpack/renderer.prod.conf.js",
    "build": "npm run build:main & npm run build:renderer",
    "dev": "npm run dev:renderer",
    "start": "electron ./app/dist/main.js",
    "pack": "cross-env ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ build --dir",
    "rebuild": "./node_modules/.bin/electron-rebuild -d=https://gh-contractor-zcbenz.cnpmjs.org/atom-shell/dist/ -m ./app"
  },
  "keywords": [],
  "build": {
    "productName": "ElectronWebpack",
    "appId": "org.develar.ElectronWebpack",
    "asar": false,
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "author": {
    "name": "Poling",
    "email": "466681097@qq.com"
  },
  "license": "ISC",
  "devDependencies": {
    "antd": "^2.10.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "dva": "^1.2.1",
    "electron": "^1.6.6",
    "electron-builder": "^17.3.1",
    "electron-is": "^2.4.0",
    "electron-rebuild": "^1.5.10",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
